{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": "off",
    "css-prefix-order": "off",
    "axe/forms": "off",
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "box-align",
          "-ms-filter"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 16.1",
    "not safari <= 16.1"
  ]
}